{
  "presets": [
    "stage-0",
    "es2015",
    "env"
  ],
  "plugins": [
    [
      "transform-es3-property-literals",
      "transform-es3-member-expression-literals",
      [
        "transform-es2015-modules-commonjs",
        {
          "loose": true
        }
      ],
      "component",
      [
        {
          "libraryName": "element-ui",
          "styleLibraryName": "~./src/editor/theme/element-ui"
        }
      ]
    ]
  ]
}